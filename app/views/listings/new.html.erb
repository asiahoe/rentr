<%= form_for @listing do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :description %>
    <%= form.text_field :description %>
  </div>

  <div>
    <%= form.label :kitchen %>
    <%= form.check_box :kitchen %>
  </div>

  <div>
    <%= form.label :number_of_room %>
    <%= form.text_field :number_of_room %>
  </div>

  <div>
    <%= form.label :price %>
    <%= form.text_field :price %>
  </div>

  <div>
    <%= form.submit "Sign in" %>
  </div>
<% end %>
